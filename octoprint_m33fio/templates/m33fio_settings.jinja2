<form class="form-horizontal">
	<div class="control-group title">
		<h4>{{_("General")}}</h4>
	</div>
	<div class="control-group">
		<div class="controls">
			<label class="checkbox" title="{{_("Disables all the Micro 3D printer specific features which allows using other types of printers" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level printerType" data-bind="checked: settings.plugins.m33fio.NotUsingAMicro3DPrinter"><span>{{_("Not using a Micro 3D printer")}}</span>
			</label>
		</div>
	</div>
	<div class="control-group">
		<div class="controls">
			<label class="checkbox" title="{{_("Logs debug information to plugin_m33fio_debug.log" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.UseDebugLogging"><span>{{_("Log debug information to plugin_m33fio_debug.log")}}</span>
			</label>
		</div>
	</div>
	<div class="control-group camera">
		<div class="controls">
			<label class="checkbox noWrap camera" title="{{_("Hosts a camera's stream and snapshot at port 4999 at /stream.mjpg and /snapshot.jpg respectively" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level hostCamera" data-bind="checked: settings.plugins.m33fio.HostCamera"><span>{{_("Host camera")}}</span>
				<select class="input-block-level short" data-bind="value: settings.plugins.m33fio.CameraPort"></select>
			</label>
		</div>
	</div>
	<div class="control-group dependant camera">
		<label class="control-label">{{_("Camera width")}}</label>
		<div class="controls">
			<div class="input-append px">
				<input type="number" step="1" min="1" max="4096" class="input-block-level" data-bind="value: settings.plugins.m33fio.CameraWidth">
				<span class="add-on">px</span>
			</div>
		</div>
	</div>
	<div class="control-group dependant camera">
		<label class="control-label">{{_("Camera height")}}</label>
		<div class="controls">
			<div class="input-append">
				<input type="number" step="1" min="1" max="4096" class="input-block-level" data-bind="value: settings.plugins.m33fio.CameraHeight">
				<span class="add-on">px</span>
			</div>
		</div>
	</div>
	<div class="control-group dependant camera">
		<label class="control-label">{{_("Camera frames/second")}}</label>
		<div class="controls">
			<div class="input-append">
				<input type="number" step="1" min="1" max="60" class="input-block-level" data-bind="value: settings.plugins.m33fio.CameraFramesPerSecond">
				<span class="add-on">fps</span>
			</div>
		</div>
	</div>
	<div class="control-group micro3d">
		<div class="controls">
			<label class="checkbox" title="{{_("Uses settings obtained from the printer to overwrite existing settings when a Micro 3D printer connects" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.AutomaticallyObtainSettings"><span>{{_("Obtain printer settings automatically when a Micro 3D printer connects")}}</span>
			</label>
		</div>
	</div>
	<div class="control-group micro3d">
		<div class="controls">
			<label class="checkbox" title="{{_("Pre‐processes each G-code command right before being sent to the printer instead of having the entire G-code file pre‐processed when being uploaded or sliced" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.PreprocessOnTheFly"><span>{{_("Pre‐process on the fly")}}</span>
			</label>
		</div>
	</div>
	<div class="control-group dependant micro3d">
		<div class="controls">
			<label class="checkbox" title="{{_("A dialog box will appear right before printing to allow changing print settings")}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.ChangeSettingsBeforePrint"><span>{{_("Change settings before print" | replace("\"", "&quot;"))}}</span>
			</label>
		</div>
	</div>
	<div class="control-group sharedLibrary micro3d">
		<div class="controls">
			<label class="checkbox" title="{{_("Uses the shared library pre‐processor instead of the Python pre‐processor to pre‐process G-code files and commands" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.UseSharedLibrary"><span>{{_("Use shared library")}}</span>
			</label>
		</div>
	</div>
	<div class="control-group micro3d">
		<div class="controls">
			<label class="checkbox" title="{{_("Allows uploading and printing models that are larger than the printer's max dimensions" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.IgnorePrintDimensionLimitations"><span>{{_("Ignore print dimension limitations")}} <span class="label label-important">{{_("Dangerous")}}</span></span>
			</label>
		</div>
	</div>
	<div class="control-group title micro3d">
		<h4>{{_("Pre‐processor stages")}}</h4>
	</div>
	<div class="control-group micro3d">
		<div class="controls">
			<label class="noCheckbox noWrap" title="{{_("Adds in mid‐print filament change commands at the start of each specified layer. Layer numbers should be seperated by a space." | replace("\"", "&quot;"))}}">
				<span class="noCheckbox">{{_("Mid‐print filament change layers")}}</span>
				<input type="text" pattern="[\d\s]*" class="input-block-level short" data-bind="value: settings.plugins.m33fio.MidPrintFilamentChangeLayers">
			</label>
		</div>
	</div>
	<div class="control-group micro3d">
		<div class="controls">
			<label class="checkbox" title="{{_("Positions the printed object in the middle of the print bed. If possible, the centered model will be repositioned if it goes out of bounds." | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.UseCenterModelPreprocessor"><span>{{_("Center model")}}</span>
			</label>
		</div>
	</div>
	<div class="control-group micro3d">
		<div class="controls">
			<label class="checkbox" title="{{_("Removes G-code commands that are not compatible with the Micro 3D printer" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.UseValidationPreprocessor"><span>{{_("Validation")}}</span>
			</label>
		</div>
	</div>
	<div class="control-group dependant micro3d">
		<div class="controls">
			<label class="checkbox" title="{{_("Removes all fan commands" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.RemoveFanCommands"><span>{{_("Remove fan commands")}}</span>
			</label>
		</div>
	</div>
	<div class="control-group dependant micro3d">
		<div class="controls">
			<label class="checkbox" title="{{_("Removes all temperature commands" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.RemoveTemperatureCommands"><span>{{_("Remove temperature commands")}}</span>
			</label>
		</div>
	</div>
	<div class="control-group micro3d">
		<div class="controls">
			<label class="checkbox" title="{{_("Adds in G-code intro and outro" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.UsePreparationPreprocessor"><span>{{_("Preparation")}}</span>
			</label>
		</div>
	</div>
	<div class="control-group dependant micro3d">
		<div class="controls">
			<label class="checkbox" title="{{_("Changes the LED's brightness to dim when a print starts and flash when a print is done" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.ChangeLedBrightness"><span>{{_("Change LED brightness")}}</span>
			</label>
		</div>
	</div>
	<div class="control-group dependant micro3d">
		<div class="controls">
			<label class="checkbox" title="{{_("Performs the bed center Z0 calibration procedure before printing. Avoid leaving the printer unmonitored until after the print starts in case the resulting calibration is inaccurate." | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.CalibrateBeforePrint"><span>{{_("Calibrate before print")}} <span class="label label-important">{{_("Caution")}}</span></span>
			</label>
		</div>
	</div>
	<div class="control-group dependant gpio micro3d">
		<div class="controls">
			<label class="checkbox noWrap gpio" title="{{_("Sets a GPIO pin high at the start of the specified layer and sets it low when the print is done" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level useGpio" data-bind="checked: settings.plugins.m33fio.UseGpio"><span>{{_("Use GPIO")}}</span>
				<input type="number" step="1" min="0" class="input-block-level short" data-bind="value: settings.plugins.m33fio.GpioPin" placeholder="{{_("GPIO pin number" | replace("\"", "&quot;"))}}">
				<input type="number" step="1" min="1" class="input-block-level short" data-bind="value: settings.plugins.m33fio.GpioLayer" placeholder="{{_("GPIO start layer" | replace("\"", "&quot;"))}}">
			</label>
		</div>
	</div>
	<div class="control-group micro3d">
		<div class="controls">
			<label class="checkbox" title="{{_("Adds dwell times and higher temperatures to the first few layers" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.UseThermalBondingPreprocessor"><span>{{_("Thermal bonding")}}</span>
			</label>
		</div>
	</div>
	<div class="control-group micro3d">
		<div class="controls">
			<label class="checkbox" title="{{_("Moves the extruder in a wave pattern during the first layer of a print to improve adhesion with the print bed" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.UseWaveBondingPreprocessor"><span>{{_("Wave bonding")}}</span>
			</label>
		</div>
	</div>
	<div class="control-group micro3d">
		<div class="controls">
			<label class="checkbox" title="{{_("Adjusts the Z value of every G-code command to account for bed leveling" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.UseBedCompensationPreprocessor"><span>{{_("Bed compensation")}} <span class="label label-important">{{_("Always internally enabled when using iMe firmware")}}</span></span>
			</label>
		</div>
	</div>
	<div class="control-group micro3d">
		<div class="controls">
			<label class="checkbox" title="{{_("Adjusts the X and Y values of G-code commands that change the current X or Y direction to account for backlash when changing directions" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.UseBacklashCompensationPreprocessor"><span>{{_("Backlash compensation")}} <span class="label label-important">{{_("Always internally enabled when using iMe firmware")}}</span></span>
			</label>
		</div>
	</div>
	<div class="control-group micro3d">
		<div class="controls">
			<label class="checkbox" title="{{_("Adjusts the X and Y values of all G-code commands based on the current height of the extruder to account for the printer's frame being skewed" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.UseSkewCompensationPreprocessor"><span>{{_("Skew compensation")}} <span class="label label-important">{{_("Always internally enabled when using iMe firmware")}}</span></span>
			</label>
		</div>
	</div>
	<div class="control-group title micro3d">
		<h4>{{_("Printer settings")}}</h4>
	</div>
	<div class="control-group micro3d" title="{{_("Amount of backlash compensation required for the X motor" | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("Backlash X")}}</label>
		<div class="controls">
			<div class="input-append">
				<input type="number" step="0.000001" min="0" max="2" class="input-block-level" data-bind="value: settings.plugins.m33fio.BacklashX">
				<span class="add-on">mm</span>
			</div>
		</div>
	</div>
	<div class="control-group micro3d" title="{{_("Amount of backlash compensation required for the Y motor" | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("Backlash Y")}}</label>
		<div class="controls">
			<div class="input-append">
				<input type="number" step="0.000001" min="0" max="2" class="input-block-level" data-bind="value: settings.plugins.m33fio.BacklashY">
				<span class="add-on">mm</span>
			</div>
		</div>
	</div>
	<div class="control-group micro3d" title="{{_("Speed of the backlash compensation" | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("Backlash speed")}}</label>
		<div class="controls">
			<div class="input-append mmPerMinute">
				<input type="number" step="0.000001" min="1" class="input-block-level" data-bind="value: settings.plugins.m33fio.BacklashSpeed">
				<span class="add-on">mm/min</span>
			</div>
		</div>
	</div>
	<div class="control-group micro3d" title="{{_("The sum of this value and the back left offset is the total vertical displacement for the back left corner. A positive vertical displacement means that the back left corner is higher than the bed's center, and a negative vertical displacement means that the back left corner is lower than the bed's center." | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("Back left orientation")}}</label>
		<div class="controls">
			<div class="input-append">
				<input type="number" step="0.000001" min="-3" max="3" class="input-block-level" data-bind="value: settings.plugins.m33fio.BackLeftOrientation">
				<span class="add-on">mm</span>
			</div>
		</div>
	</div>
	<div class="control-group micro3d" title="{{_("The sum of this value and the back right offset is the total vertical displacement for the back right corner. A positive vertical displacement means that the back right corner is higher than the bed's center, and a negative vertical displacement means that the back right corner is lower than the bed's center." | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("Back right orientation")}}</label>
		<div class="controls">
			<div class="input-append">
				<input type="number" step="0.000001" min="-3" max="3" class="input-block-level" data-bind="value: settings.plugins.m33fio.BackRightOrientation">
				<span class="add-on">mm</span>
			</div>
		</div>
	</div>
	<div class="control-group micro3d" title="{{_("The sum of this value and the front right offset is the total vertical displacement for the front right corner. A positive vertical displacement means that the front right corner is higher than the bed's center, and a negative vertical displacement means that the front right corner is lower than the bed's center." | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("Front right orientation")}}</label>
		<div class="controls">
			<div class="input-append">
				<input type="number" step="0.000001" min="-3" max="3" class="input-block-level" data-bind="value: settings.plugins.m33fio.FrontRightOrientation">
				<span class="add-on">mm</span>
			</div>
		</div>
	</div>
	<div class="control-group micro3d" title="{{_("The sum of this value and the front left offset is the total vertical displacement for the front left corner. A positive vertical displacement means that the front left corner is higher than the bed's center, and a negative vertical displacement means that the front left corner is lower than the bed's center." | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("Front left orientation")}}</label>
		<div class="controls">
			<div class="input-append">
				<input type="number" step="0.000001" min="-3" max="3" class="input-block-level" data-bind="value: settings.plugins.m33fio.FrontLeftOrientation">
				<span class="add-on">mm</span>
			</div>
		</div>
	</div>
	<div class="control-group micro3d" title="{{_("The vertical displacement for the entire bed. A positive vertical displacement means that the entire bed is higher than it is, and a negative vertical displacement means that the entire bed is lower than it is." | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("Bed height offset")}}</label>
		<div class="controls">
			<div class="input-append">
				<input type="number" step="0.000001" class="input-block-level" data-bind="value: settings.plugins.m33fio.BedHeightOffset">
				<span class="add-on">mm</span>
			</div>
		</div>
	</div>
	<div class="control-group micro3d" title="{{_("The sum of this value and the back left orientation is the total vertical displacement for the back left corner. A positive vertical displacement means that the back left corner is higher than the bed's center, and a negative vertical displacement means that the back left corner is lower than the bed's center." | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("Back left offset")}}</label>
		<div class="controls">
			<div class="input-append">
				<input type="number" step="0.000001" class="input-block-level" data-bind="value: settings.plugins.m33fio.BackLeftOffset">
				<span class="add-on">mm</span>
			</div>
		</div>
	</div>
	<div class="control-group micro3d" title="{{_("The sum of this value and the back right orientation is the total vertical displacement for the back right corner. A positive vertical displacement means that the back right corner is higher than the bed's center, and a negative vertical displacement means that the back right corner is lower than the bed's center." | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("Back right offset")}}</label>
		<div class="controls">
			<div class="input-append">
				<input type="number" step="0.000001" class="input-block-level" data-bind="value: settings.plugins.m33fio.BackRightOffset">
				<span class="add-on">mm</span>
			</div>
		</div>
	</div>
	<div class="control-group micro3d" title="{{_("The sum of this value and the front right orientation is the total vertical displacement for the front right corner. A positive vertical displacement means that the front right corner is higher than the bed's center, and a negative vertical displacement means that the front right corner is lower than the bed's center." | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("Front right offset")}}</label>
		<div class="controls">
			<div class="input-append">
				<input type="number" step="0.000001" class="input-block-level" data-bind="value: settings.plugins.m33fio.FrontRightOffset">
				<span class="add-on">mm</span>
			</div>
		</div>
	</div>
	<div class="control-group micro3d" title="{{_("The sum of this value and the front left orientation is the total vertical displacement for the front left corner. A positive vertical displacement means that the front left corner is higher than the bed's center, and a negative vertical displacement means that the front left corner is lower than the bed's center." | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("Front left offset")}}</label>
		<div class="controls">
			<div class="input-append">
				<input type="number" step="0.000001" class="input-block-level" data-bind="value: settings.plugins.m33fio.FrontLeftOffset">
				<span class="add-on">mm</span>
			</div>
		</div>
	</div>
	<div class="control-group micro3d" title="{{_("Height of an external object that's placed on the bed, such as a heatbed" | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("External bed height")}}</label>
		<div class="controls">
			<div class="input-append">
				<input type="number" step="0.000001" min="0" max="50" class="input-block-level" data-bind="value: settings.plugins.m33fio.ExternalBedHeight">
				<span class="add-on">mm</span>
			</div>
		</div>
	</div>
	<div class="control-group micro3d">
		<div class="controls">
			<label class="checkbox" title="{{_("Expands printable region to include everywhere the extruder can move" | replace("\"", "&quot;"))}}">
				<input type="checkbox" class="input-block-level" data-bind="checked: settings.plugins.m33fio.ExpandPrintableRegion"><span>{{_("External bed expands printable region to max")}}</span>
			</label>
		</div>
	</div>
	<div class="control-group micro3d" title="{{_("Total skew of the X axis from the lowest to the highest Z position that the extruder can go when in the center of the bed. If your using an external bed then make sure that the lowest and highest Z positions are when the external bed is attached." | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("Skew X")}}</label>
		<div class="controls">
			<div class="input-append">
				<input type="number" step="0.000001" min="-40" max="40" class="input-block-level" data-bind="value: settings.plugins.m33fio.SkewX">
				<span class="add-on">mm</span>
			</div>
		</div>
	</div>
	<div class="control-group micro3d" title="{{_("Total skew of the Y axis from the lowest to the highest Z position that the extruder can go when in the center of the bed. If your using an external bed then make sure that the lowest and highest Z positions are when the external bed is attached." | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("Skew Y")}}</label>
		<div class="controls">
			<div class="input-append">
				<input type="number" step="0.000001" min="-40" max="40" class="input-block-level" data-bind="value: settings.plugins.m33fio.SkewY">
				<span class="add-on">mm</span>
			</div>
		</div>
	</div>
	<div class="control-group micro3d" title="{{_("Extruder's printing temperature" | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("Filament temperature")}}</label>
		<div class="controls">
			<div class="input-append degreesCelsius">
				<input type="number" step="1" min="150" max="315" class="input-block-level" data-bind="value: settings.plugins.m33fio.FilamentTemperature">
				<span class="add-on">°C</span>
			</div>
		</div>
	</div>
	<div class="control-group heatbed micro3d" title="{{_("Heatbed's printing temperature" | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("Heatbed temperature")}}</label>
		<div class="controls">
			<div class="input-append degreesCelsius">
				<input type="number" step="1" min="0" max="110" class="input-block-level" data-bind="value: settings.plugins.m33fio.HeatbedTemperature">
				<span class="add-on">°C</span>
			</div>
		</div>
	</div>
	<div class="control-group micro3d" title="{{_("Type of filement being printed" | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("Filament type")}}</label>
		<div class="controls">
			<select class="input-block-level" data-bind="value: settings.plugins.m33fio.FilamentType">
				<option value="ABS">{{_("%(type)s (Recommended %(temperature)s)", type = "ABS", temperature = "275°C")}}</option>
				<option value="PLA">{{_("%(type)s (Recommended %(temperature)s)", type = "PLA", temperature = "215°C")}}</option>
				<option value="HIPS">{{_("%(type)s (Recommended %(temperature)s)", type = "HIPS", temperature = "265°C")}}</option>
				<option value="FLX">{{_("%(type)s (Recommended %(temperature)s)", type = "FLX", temperature = "220°C")}}</option>
				<option value="TGH">{{_("%(type)s (Recommended %(temperature)s)", type = "TGH", temperature = "220°C")}}</option>
				<option value="CAM">{{_("%(type)s (Recommended %(temperature)s)", type = "CAM", temperature = "215°C")}}</option>
				<option value="ABS-R">{{_("%(type)s (Recommended %(temperature)s)", type = "ABS‐R", temperature = "240°C")}}</option>
				<option value="OTHER">{{_("Other")}}</option>
			</select>
		</div>
	</div>
	<div class="control-group empty micro3d">
		<h4><span class="label label-important">{{_("Changes to speed limits require restarting the printer to<br>take effect when using M3D or M3D Mod firmware")}}</span></h4>
	</div>
	<div class="control-group micro3d" title="{{_("X motor's speed limit" | replace("\"", "&quot;"))}}">
		<label class="control-label">{{_("Speed limit X")}}</label>
		<div class="controls">
			<div class="input-append mmPerMinute">
				<input type="number" step="1" min="120" max="4800" class="input-block-level" data-bind="value: settings.plugins.m33fio.SpeedLimitX">
